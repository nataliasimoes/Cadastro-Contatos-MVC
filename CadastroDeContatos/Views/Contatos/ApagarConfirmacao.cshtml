@model ContatoModel
@{
    ViewData["Title"] = "Apagar contato";
}


<div class="text-center">
    <h1>Deseja apagar o contato (@Model.Nome)?</h1>
</div>


<a class="btn btn-primary" role="button" asp-controller="Contatos" asp-action="Index">Cancelar</a>
<a class="btn btn-danger" role="button" asp-controller="Contatos" asp-action="Apagar" asp-route-id="@Model.Id">Excluir</a>